@model List<ResultCategoryDto>
<div class="container-fluid ExploreTour py-5">
    <div class="container py-5">
        <div class="mx-auto text-center mb-5" style="max-width: 900px;">
            <h5 class="section-title px-3">Turu Keşfet</h5>
            <h1 class="mb-4">Dünya</h1>
            <p class="mb-0">
              Birbirinden farklı kategorilerdeki turların tadını çıkarınnn
            </p>
        </div>
        <div class="tab-class text-center">
        
         
                <div id="NationalTab-1" class="tab-pane fade show p-0 active">
                    <div class="row g-4">
                        @foreach (var item in Model)
                        {
                            <div class="col-md-6 col-lg-4">
                            <div class="national-item">
                                <img src="@item.imageUrl" class="img-fluid w-100 rounded" alt="Image">
                                <div class="national-content">
                                    <div class="national-info">
                                        <h5 class="text-white text-uppercase mb-2">@item.categoryName</h5>
                                        <a href="#" class="btn-hover text-white">Tüm Yerleri Görün<i class="fa fa-arrow-right ms-2"></i></a>
                                    </div>
                                </div>
                                <div class="national-plus-icon">
                                    <a href="#" class="my-auto"><i class="fas fa-link fa-2x text-white"></i></a>
                                </div>
                            </div>
                        </div>
                        }
                        
                        
                    </div>
                </div>
                
          
        </div>
    </div>
</div>